package com.java;

public class R1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MyThread2 m = new MyThread2();
		Thread t = new Thread(m);
		t.start();
	
		Runnable r = ()-> {
			for(int i = 0; i <= 5; i++) {
				System.out.println(i);
			}
		};
		Thread t1 = new Thread(r);
		t1.start();

}
class MyThread2 implements Runnable {

public void run() {
	for(int i = 1; i <= 5; i++) {
		System.out.println(i);
	}
}
}

