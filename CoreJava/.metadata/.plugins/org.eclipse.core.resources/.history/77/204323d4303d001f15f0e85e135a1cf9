package com.java;

public class Singleton {

	public static void main(String[] args) {
		

	}

}
class Singleton1 {
	private static Singleton1 instance;
	public Singleton1() {
		
	}
	public static Singleton1 getInstance() {
		if(instance ==null) {
			instance = new Singleton1();
		}
		return instance;
	}
}