	for(int i = 0; i <= s1.length()-1; i++) {
		char ch = s1.charAt(i);
		if(s1.contains(s2)) {
			System.out.println(s2 + " first String present in second");
			break;
		}
		else {
			System.out.println("nope");
			break;
		}
	}

	}

