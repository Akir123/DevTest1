class Optional {
	public Optional<String> username(Integer id) {
		String name = null;
		if(id == 100) {
			return "Raju";
		}
		else if(id == 200) {
			return "Ashu";
		}
		else if(id == 300) {
			return "Aman";
		}
		return Optional.ofNullable(name);
	}
}