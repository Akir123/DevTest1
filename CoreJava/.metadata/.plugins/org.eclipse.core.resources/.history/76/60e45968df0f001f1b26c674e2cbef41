package Core;

public class X {

	public static void main(String[] args) {
		int num = 525;
		int temp;
		int rev = 0;
		int original = num;
		for(int i = 1; i <= num; i++) {
			temp =num % 10;
			rev = rev * 10 + temp;
			num = num / 10;
		}
		if(original == rev)
		System.out.println(rev);
		else 
			System.out.println(rev + " not");
		
	}

}
