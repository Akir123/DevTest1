package com.java;

public class R1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MyThread2 m = new MyThread2();
		Thread t = new Thread(m);
		t.start();

	}

}
class MyThread2 implements Runnable {
@Override	
public void run() {
	for(int i = 1; i <= 5; i++) {
		System.out.println(i);
	}
}
}
