package Core;


public class T {

	public static void main(String[] args) {
		String s = "himalaya";
		String s1 = "";
		for(int i = s.length()-1; i >= 0; i--) {
			s1 = s1 + s.charAt(i);
			if(s.equals(s1)) {
				System.out.println(s1 + "palindrome");
				break;
			}
			else {
				System.out.println(s1 +"non palindrome");
				break;
			}
		}

	}

}
