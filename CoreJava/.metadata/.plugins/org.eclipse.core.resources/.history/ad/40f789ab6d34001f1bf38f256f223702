package com.java;

import java.util.Scanner;

public class Power {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int b = sc.nextInt();
		int p = sc.nextInt();
		int x = (int) Math.pow(p, b);
		System.out.println(x);
		
	}


}