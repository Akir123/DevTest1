package com.java;

import java.util.Optional;

public class OptionalClass1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
class Optio {
public String<Integer> getUsername(Integer age) {
	String name = null;
	if(age == 20) {
		return "adi";
	}
	else if(age == 40) {
		return "ashu";
	}
	return Optional.ofNullable(name);
}
}